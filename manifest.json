{
	"manifest_version": 2,
	"name": "Confiner",
	"description": "Confine every site to its own container",
	"version": "1.1.0",
	"icons": {
		"48": "icons/48.png"
	},
	"permissions": [
		"<all_urls>",
		"cookies",
		"contextualIdentities",
		"storage",
		"webRequestBlocking",
		"webRequest"
	],
	"background": {
		"scripts": ["background.js"]
	}
}
